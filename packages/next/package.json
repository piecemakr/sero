{
  "name": "@sero/nextjs",
  "version": "0.1.0",
  "description": "Next.js integration for Sero route transitions",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "Troy Hancock",
  "homepage": "https://github.com/troyhancock/sero#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/troyhancock/sero.git",
    "directory": "packages/next"
  },
  "bugs": {
    "url": "https://github.com/troyhancock/sero/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "dev": "tsc -w -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "prepublishOnly": "pnpm -w build",
    "size": "size-limit",
    "analyze": "npx bundlesize"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "7 KB"
    }
  ],
  "keywords": [
    "nextjs",
    "app router",
    "transitions",
    "view-transitions",
    "navigation",
    "sero"
  ],
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "next": ">=13.0.0"
  },
  "dependencies": {
    "@sero/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4"
  }
}
